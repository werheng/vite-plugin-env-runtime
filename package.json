{
  "name": "vite-plugin-env-runtime",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@9.0.1",
  "description": "Configure environment variables on runtime.",
  "author": "Werheng <werheng.zhang@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/werheng/vite-plugin-env-runtime",
  "bugs": {
    "url": "https://github.com/werheng/vite-plugin-env-runtime/issues"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "env",
    "runtime",
    "config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.9"
  },
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "play": "cd playground && vite",
    "play:build": "cd playground && vite build",
    "play:preview": "cd playground && vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "commit": "git-cz",
    "release": "bumpp -r",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deps": "pnpx taze -r",
    "deps:up": "pnpx taze -wr"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "dotenv": "^16.4.5",
    "magic-string": "^0.30.9",
    "minimatch": "^9.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
